    <script type="text/javascript">


        new Morris.Area({
            element: 'kt_morris_2',
            data: [{
                y: '2010',
                a: 50,
                b: 100
            },
            {
                y: '2011',
                a: 75,
                b: 65
            },
            {
                y: '2012',
                a: 50,
                b: 40
            },
            {
                y: '2013',
                a: 75,
                b: 65
            }

            ],
            xkey: 'y',
            ykeys: ['a', 'b'],
            labels: ['OPD Earnings', 'OPD Earnings'],
            lineColors: ['#4eaefd', '#e64833'],
            pointFillColors: ['#9ad1fe', ' #ec7362']
        });

        var demo8 = function () {
            var data = [
                { label: "Credit Card", data: 40, color: "#01b091" },

                { label: "Insurance", data: 60, color: "#038dfd" },

            ];

            $.plot($("#kt_flotcharts_8"), data, {
                series: {
                    pie: {
                        show: true
                    }
                }
            });
        }

        jQuery(document).ready(function () {
            demo8();
        });

    </script>
    <script src="~/Content/assets/vendors/custom/flot/flot.bundle.js" type="text/javascript"></script>

