    <script type="text/javascript">
        new Morris.Bar({
            element: 'kt_morris_3',
            data: [{
                y: '',
                a: 100,
                b: 90
            },
            {
                y: '',
                a: 75,
                b: 65
            }
            ],
            xkey: 'y',
            ykeys: ['a', 'b'],
            labels: ['Cardiology', 'Pulmonology'],
            barColors: ['#01b091', '#038dfd'],
            resize: true,
            redraw: true
        });

        new Morris.Bar({
            element: 'MonthlySales',
            data: [{
                y: 'Jan',
                a: 100,
                b: 90
            },
            {
                y: 'Feb',
                a: 75,
                b: 65
            }
            ],
            xkey: 'y',
            ykeys: ['a', 'b'],
            labels: ['', ''],
            barColors: ['#01b091', '#038dfd']
        });

        var demo8 = function () {
            var data = [
                { label: "Anesthetics", data: 60, color: "#01b091" },

                { label: "Anti-Allergics", data: 40, color: "#038dfd" },

            ];

            $.plot($("#kt_flotcharts_8"), data, {
                series: {
                    pie: {
                        show: true
                    }
                }
            });
        }

        jQuery(document).ready(function () {
            demo8();
        });

    </script>

