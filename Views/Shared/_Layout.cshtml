    <script>
        function starMouseEnter(starNumber) {
        if($("#starClicked").val() != "Clicked") {
        for(var j=1; j<=starNumber; j++) {
        $("#star"+j).addClass("checked-feedback");
        }
        }
        }
        function starMouseLeave() {
        if($("#starClicked").val() != "Clicked") {
        for(var j=1; j<=5; j++) {
        if($("#star"+j).hasClass("checked-feedback"))
        $("#star"+j).removeClass("checked-feedback");
        }
        }
        }
        function starClicked(starNumber) {
        $("#starClicked").val("Clicked");
        $("#feedbackformdiv").removeClass("d-none");
        }
        function clearClicked() {
        $("#starClicked").val("");
        $("#feedbackformdiv").addClass("d-none");
        $("#feedbackTextArea").val("");
        for(var j=1; j<=5; j++) {
        if($("#star"+j).hasClass("checked-feedback"))
        $("#star"+j).removeClass("checked-feedback");
        }
        }

        

    </script>

