@using (Html.BeginForm())
{
    <!-- begin:: Content Head -->
    <div class="kt-subheader   kt-grid__item" id="kt_subheader">
        <div class="kt-subheader__main">
            <h3 class="kt-subheader__title"><span><img style="width:30px" class="mr-3" src="~/Content/assets/media/med360/patient.png"></span>Patient Encounter</h3>

        </div>
        <div class="kt-subheader__toolbar">
            <button type="button" class="btn btn-primary"><span><i class="fa fa-pause"></i>Hold</span></button>
            <button type="button" class="btn btn-danger"><span><i class="fa fa-stop"></i>End</span></button>
            <button type="button" class="btn btn-info"><span><i class="fa fa-eye"></i>View</span></button>
        </div>
    </div>
