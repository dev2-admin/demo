
@{
    ViewBag.PageName = "Patient Encounter";

}
@section Styles
{

    <link href="@Url.Content("~/Content/assets/vendors/custom/datatables/datatables.bundle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/assets/vendors/custom/datatables/patient-encounter.css")" rel="stylesheet" type="text/css" />

    <style type="text/css">


        /*.mainPatientEncounter > svg rect{
            width:60px !important;
            margin: 0 auto !important
        }*/

        .kt-portlet {
            margin-bottom: 10px;
        }

            .kt-portlet .kt-portlet__head {
                min-height: 40px;
                padding: 5px 5px;
                background-color: #038dfd;
            }

            .kt-portlet .kt-portlet__body {
                padding: 5px 5px;
            }


            .kt-portlet .kt-portlet__head.kt-portlet__head--fit {
                background-color: #038dfd;
            }

        .kt-form {
            overflow: auto;
        }
        /*.partialViewWrapper {

            overflow-y: auto;
            overflow-x:hidden auto !important;
        }*/


        .col-1, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9, .col-10, .col-11, .col-12, .col, .col-auto, .col-sm-1, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-sm-10, .col-sm-11, .col-sm-12, .col-sm, .col-sm-auto, .col-md-1, .col-md-2, .col-md-3, .col-md-4, .col-md-5, .col-md-6, .col-md-7, .col-md-8, .col-md-9, .col-md-10, .col-md-11, .col-md-12, .col-md, .col-md-auto, .col-lg-1, .col-lg-2, .col-lg-3, .col-lg-4, .col-lg-5, .col-lg-6, .col-lg-7, .col-lg-8, .col-lg-9, .col-lg-10, .col-lg-11, .col-lg-12, .col-lg, .col-lg-auto, .col-xl-1, .col-xl-2, .col-xl-3, .col-xl-4, .col-xl-5, .col-xl-6, .col-xl-7, .col-xl-8, .col-xl-9, .col-xl-10, .col-xl-11, .col-xl-12, .col-xl, .col-xl-auto {
            /*padding-right: 5px;
            padding-left: 5px;*/
        }
        .gutter {
            cursor: e-resize;
            border-radius: 10px;
            align-items: center;
            justify-content: center;
            display:flex;
            z-index: 99;
        }
            .gutter:hover {
                cursor: e-resize;
                background: #ececf2;
                z-index:99;
            }
            .gutter::before {
                content: "";
                width: 0;
                height: 0;
                border-top: 3px solid transparent;
                border-right: 6px solid #4f4f51;
                border-bottom: 3px solid transparent;
                display: flex;
                margin-right:1px;
            }
            .gutter::after {
                content: "";
                width: 0;
                height: 0;
                border-top: 3px solid transparent;
                border-left: 6px solid #4f4f51;
                border-bottom: 3px solid transparent;
                display: flex;
                margin-left: 1px;
            }
        
    </style>
}

